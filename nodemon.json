{
  "watch": ["src", "docs"],
  "ext": ".ts,.js,.json,.yaml",
  "exec": "ts-node -r tsconfig-paths/register ./src/index.ts",
  "env": {
    "NODE_ENV": "development"
  },
  "ignore": [
    "coverage/*",
    "dist/*",
    "docs/*",
    "node_modules/*",
    "prisma/*",
    "public/*",
    "test/*"
  ]
}
